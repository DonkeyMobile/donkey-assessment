{
  "name": "backend",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "backend": "bin/backend.js"
  },
  "scripts": {
    "audit": "better-npm-audit audit",
    "build": "tsc",
    "lint": "eslint bin lib src",
    "watch": "tsc -w",
    "test": "vitest run",
    "generate-openapi-models": "typed-openapi ./openapi.yaml -o ./src/handlers/generated/rest-model.ts -r zod",
    "cdk": "cdk",
    "cdk:deploy": "cdk deploy --require-approval never",
    "cdk:watch": "cdk watch --require-approval never",
    "cdk:synth:template": "npm run cdk synth > cdk.out/template.json",
    "dev": "npm run build && npm run cdk:synth:template"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.876.0",
    "@eslint/js": "^9.34.0",
    "@types/aws-lambda": "^8.10.152",
    "@types/jest": "^30.0.0",
    "@types/mustache": "^4.2.6",
    "@types/node": "24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "aws-cdk": "2.1026.0",
    "eslint": "^9.34.0",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typed-openapi": "^2.1.2",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.41.0",
    "zod": "^4.1.3"
  },
  "dependencies": {
    "@aws-lambda-powertools/commons": "^2.25.1",
    "@aws-lambda-powertools/logger": "^2.25.2",
    "@aws-lambda-powertools/parameters": "^2.25.2",
    "@aws-lambda-powertools/parser": "^2.25.2",
    "@aws-sdk/lib-dynamodb": "^3.876.0",
    "aws-cdk-lib": "2.212.0",
    "better-npm-audit": "^3.11.0",
    "constructs": "^10.4.2",
    "temporal-polyfill": "^0.3.0",
    "ulid": "^3.0.1",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  }
}
